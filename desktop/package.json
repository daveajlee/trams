{
  "name": "trams-frontend",
  "version": "0.6.0",
  "main": "src/electron/main-forge.js",
  "scripts": {
    "ng": "ng",
    "serve": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-electron": "ng build --base-href . && cp src/electron/* dist/",
    "electron": "npm run build-electron && ./node_modules/.bin/electron dist/",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make --platform darwin & electron-forge make --platform linux & electron-forge make --platform win32",
    "make-pkg": "dotenv -- electron-forge make --arch=x64,arm64 --platform mas",
    "publish": "electron-forge publish"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "^17.3.8",
    "@angular/animations": "~17.3.11",
    "@angular/cdk": "^17.3.10",
    "@angular/common": "~17.3.11",
    "@angular/compiler": "~17.3.11",
    "@angular/core": "~17.3.11",
    "@angular/forms": "~17.3.11",
    "@angular/material": "^17.3.10",
    "@angular/platform-browser": "~17.3.11",
    "@angular/platform-browser-dynamic": "~17.3.11",
    "@angular/router": "~17.3.11",
    "@electron/osx-sign": "^1.3.0",
    "@fortawesome/angular-fontawesome": "^0.14.1",
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "bootstrap": "^5.3.1",
